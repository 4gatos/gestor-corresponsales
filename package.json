{
  "name": "gestor-corresponsales",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "draft-js": "^0.10.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-draft-wysiwyg": "^1.12.13",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "stylus -c src/stylus/main.styl --out src/static/css/main.css",
    "watch-css": "npm run build-css && stylus -c -w src/stylus/main.styl --out src/static/css/main.css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
